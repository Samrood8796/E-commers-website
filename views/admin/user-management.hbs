<section>
  <div class="container-fluid ">
    <div class="row px-xl-5 pt-5">
    <div class=" table-responsive mb-5">
    <table class="table text-center" id="productsTable">
      <thead class="thead-light">
        <tr>
          <th scope="col">No</th>
          <th scope="col">Name</th>
          <th scope="col">Email</th>
          <th scope="col">Phone No</th>
          <th scope="col">Block / Unblock</th>
        </tr>
      </thead>
      <tbody>
        {{#each users}}
        <tr>
          <td scope="row">{{inc @index}}</td>
          <td>{{this.name}}</td>
          <td>{{this.email}}</td>
          <td>{{this.phone}}</td>
          <td>
            {{#if this.isblocked}}
            <a href="/admin/unblock-user/{{this._id}}" class="btn btn-success">UnBlock</a>
            {{else}}
            <a href="/admin/block-user/{{this._id}}" class="btn btn-danger">Block</a>
            {{/if}}
          </td>
        </tr>
        {{/each}}
      </tbody>
    </table>
  </div>
    </div>
  </div>
</section>